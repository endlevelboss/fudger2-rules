weaponStats =ruleset
        {
            Stats= table
            {
                Tech=integer
                {}
                Damage=string
                {}
                Ammo=integer
                {}
                Range=string
                {}
                Notes=string
                {}
            }
        }
armourStats= ruleset
        {
            Stats= table
            {
                Tech=integer
                {}
                Bonus=string
                {}
