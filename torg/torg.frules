                {}
                Notes=string
                {}
            }
        }
spellStats =ruleset
{
  Stats= table
  {
      'Axiom level'=integer
      {}
      'Type'=string
      {}
      'Skill'=string
      {}
      'Casting time'=string
      {}
      'DN'=string
