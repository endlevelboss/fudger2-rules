

mapSpellevels(spellevels)=
 (map (((x)->
       (classSpellevel((toInt((x.Key))),
       
       (map(((y)->
         (spellsForLevel((toInt((y.Key))),((y.Value).'Number of spells')))
                      ),(toList(((x.Value).levels))))
                       )
       ))),
       (toList (spellevels))
       )
 )

